{
    "name": "taivas-apm/taivas-apm-laravel",
    "description": "Application Performance Monitoring for Laravel",
    "keywords": ["laravel", "apm", "monitoring", "analytics"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Mohr",
            "email": "p.mohr@sopamo.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*|^7.0",
        "illuminate/http": "5.5.*|5.6.*|5.7.*|5.8.*|^7.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^7.0",
        "lcobucci/jwt": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.1",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "TaivasAPM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TaivasAPM\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaivasAPM\\TaivasAPMServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
